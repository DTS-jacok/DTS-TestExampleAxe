    <Cell><Data ss:Type="String">get.exists</Data></Cell>
    <Cell><Data ss:Type="String">value = %REFERENCE%WebEdit(&quot;&amp;desc&amp;&quot;).MyExists</Data></Cell>
    <Cell><Data ss:Type="String">Determine if the %object% object exists</Data></Cell>
   </Row>
   <Row>
