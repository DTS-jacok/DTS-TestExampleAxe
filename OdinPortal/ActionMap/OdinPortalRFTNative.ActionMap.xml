    <Cell><Data ss:Type="String">Type the key sequence %data% at the keyboard.</Data></Cell>
   </Row>
   <Row>
    <Cell ss:Index="2"><Data ss:Type="String">Html.INPUT.text</Data></Cell>
    <Cell><Data ss:Type="String">get.exists</Data></Cell>
